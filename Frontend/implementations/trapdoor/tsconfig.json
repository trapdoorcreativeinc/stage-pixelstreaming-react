{
  "compilerOptions": {
    "outDir": "./dist",
    "noImplicitAny": true,
    "module": "es6",
    "esModuleInterop": true,
    "target": "es5",
    "moduleResolution": "node",
    "sourceMap": false,
    "allowJs": true,
    "declaration": false,
    "jsx": "react-jsx"
  },
  "lib": ["es2015"],
  "include": ["./src/*.tsx"],
  "types": [
    "webpack-env",
    "node"
  ],
  "typeRoots": [
    "./node_modules/@types"
  ]
}
